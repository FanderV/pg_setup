Table view1 as "view1: Проверка по ФИО" {
  first_name VARCHAR(50)
  middle_name VARCHAR(50)
  last_name VARCHAR(50)
}

Table view2 as "view2: Все работающие дети" {
  id INT
  first_name VARCHAR(50)
  middle_name VARCHAR(50)
  last_name VARCHAR(50)
  birth_year INT
  gender CHAR(1)
  monthly_income NUMERIC(10,2)
  twin BOOLEAN
  father_id INT
  mother_id INT
}

Table view3 as "view3: Мужья с доходом выше жены" {
  husband_name VARCHAR(50)
  wife_name VARCHAR(50)
  husband_income NUMERIC(10,2)
  wife_income NUMERIC(10,2)
}

Table view4 as "view4: Люди без дохода до 1990 года" {
  first_name VARCHAR(50)
  last_name VARCHAR(50)
  birth_year INT
  monthly_income NUMERIC(10,2)
}

Table view5 as "view5: Число семей с близнецами" {
  families_with_twins INT
}
